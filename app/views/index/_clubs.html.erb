<h1>Listing clubs</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Address</th>
    <th>Info</th>
    <th>Capacity</th>
    <th>Img url</th>
    <th>City</th>
  </tr>

<% clubs.each do |club| %>
  <tr>
    <td><%=h club.name %></td>
    <td><%=h club.address %></td>
    <td><%=h club.info %></td>
    <td><%=h club.capacity %></td>
    <td><%=h club.img_url %></td>
    <td><%=h club.city_id %></td>
    <td><%= link_to 'Show', club %></td>
    <td><%= link_to 'Edit', edit_club_path(club) %></td>
    <td><%= link_to 'Destroy', club, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New club', new_club_path %>